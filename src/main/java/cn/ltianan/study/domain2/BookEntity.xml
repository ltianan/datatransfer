<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="cn.ltianan.study.domain2.BookEntity">
        <table name="book" schema="public"/>
        <attributes>
            <id name="bookId">
                <column name="book_id" precision="-1"/>
            </id>
            <basic name="oriDeptId">
                <column name="ori_dept_id" precision="-1"/>
            </basic>
            <basic name="nowDeptId">
                <column name="now_dept_id" precision="-1"/>
            </basic>
            <basic name="oriLocalId">
                <column name="ori_local_id" precision="-1"/>
            </basic>
            <basic name="nowLocalId">
                <column name="now_local_id" precision="-1"/>
            </basic>
            <basic name="bookBarcode">
                <column name="book_barcode" length="64"/>
            </basic>
            <basic name="bookStatus">
                <column name="book_status" length="8"/>
            </basic>
            <basic name="bookPrice">
                <column name="book_price" precision="2"/>
            </basic>
            <basic name="bookSetPrice">
                <column name="book_set_price" precision="2"/>
            </basic>
            <basic name="indexId">
                <column name="index_id" precision="-1"/>
            </basic>
            <basic name="bookSource">
                <column name="book_source" nullable="false" length="64"/>
            </basic>
            <basic name="bookMediaType">
                <column name="book_media_type" nullable="false" length="64"/>
            </basic>
            <basic name="bookDescription">
                <column name="book_description" nullable="false" length="200"/>
            </basic>
            <basic name="shouldBackTime">
                <column name="should_back_time" nullable="false" precision="-1"/>
            </basic>
            <basic name="bookUuid">
                <column name="book_uuid" nullable="false" length="64"/>
            </basic>
            <basic name="oriPlaceNo">
                <column name="ori_place_no" nullable="false" length="64"/>
            </basic>
            <basic name="nowPlaceNo">
                <column name="now_place_no" nullable="false" length="64"/>
            </basic>
            <basic name="nano">
                <column name="nano" nullable="false" length="200"/>
            </basic>
            <basic name="createBy">
                <column name="create_by" nullable="false" length="64"/>
            </basic>
            <basic name="createTime">
                <column name="create_time" nullable="false" precision="-1"/>
            </basic>
            <basic name="updateBy">
                <column name="update_by" nullable="false" length="64"/>
            </basic>
            <basic name="updateTime">
                <column name="update_time" nullable="false" precision="-1"/>
            </basic>
            <basic name="sshid">
                <column name="sshid" nullable="false"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
